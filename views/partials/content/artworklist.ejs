<article class="artworklist">
  <h1>Artwork on display</h1>
  <p>Check the gallary as i it took time to download over the google.</p>
  <div class="pixgrid">
    <% if (artwork.length > 0) { %>
      <% for (i=0; i<artwork.length; i++) { %>
        <img src="/images/artwork/<%= artwork[i] %>" alt="Artwork <%= i %>">
      <% } %>
    <% } %>
  </div>
</article>